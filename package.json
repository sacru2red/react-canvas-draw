  "repository": "https://github.com/sacru2red/react-canvas-draw",
  "bugs": {
    "url": "https://github.com/sacru2red/react-canvas-draw/issues"
  },
  "keywords": [
    "react-component",
    "react",
    "reactjs",
    "canvas",
    "drawing"
  ],
  "volta": {
    "node": "10.24.1",
    "npm": "6.14.12"
  }
}
